    "build:sitemap": "pnpx svelte-sitemap --domain https://contentauthenticity.org -o dist",
    "preview": "vite preview",
    "test": "run-p test:unit test:e2e",
    "test:unit": "vitest run",
    "test:watch:unit": "vitest",
    "test:e2e": "HOST_PORT=$(get-port) FIXTURES_PORT=$(get-port) TEST_IMAGE_SERVICE_PORT=$(get-port) playwright test",
    "test:watch:e2e": "nodemon -w e2e -e 'ts' -x 'pnpm test:e2e'",
    "test-image-service": "pnpm c2pa-test-image-service --config e2e/c2pa-test-image-service.config.ts",
    "test-image-service:watch": "nodemon -w e2e/c2pa-test-image-service.config.ts -x 'pnpm run test-image-service'",
    "lint": "eslint .",
    "hash-pem": "node etc/scripts/hash-pem.js",
    "check": "pnpm lint && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
    "format": "prettier --plugin-search-dir . --write .",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006 --no-open",
    "build:storybook": "storybook build"
  },
